{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "editor.rulers": [120],
  "python.formatting.autopep8Args": [
    "--max-line-length",
    "120",
    "--experimental"
  ],
  "python.sortImports.args": ["-l", "120"],
  "restructuredtext.confPath": "${workspaceFolder}/docs",
  "cSpell.words": [
    "aakash",
    "auto",
    "autoclass",
    "autofunction",
    "chdir",
    "chmod",
    "conda",
    "contextlib",
    "csvs",
    "dataset",
    "datasets",
    "deps",
    "env",
    "envfile",
    "envs",
    "extension",
    "favicon",
    "fname",
    "get",
    "getcwd",
    "getvalue",
    "iopub",
    "ipykernel",
    "ipynb",
    "ipython",
    "iread",
    "isdir",
    "isfile",
    "iwrite",
    "jovian",
    "jovianrc",
    "juptyer",
    "keycodes",
    "load",
    "macos",
    "maxdepth",
    "mixpanel",
    "mnist",
    "nargs",
    "nbconfig",
    "nbext",
    "nbextension",
    "nbextensions",
    "pfname",
    "pkgs",
    "popen",
    "pyyaml",
    "quickstart",
    "rcdata",
    "rcfile",
    "recommonmark",
    "rmtree",
    "s",
    "shutil",
    "siddhant",
    "sigmasix",
    "splitlines",
    "sqlite",
    "str",
    "structify",
    "toctree",
    "tqdm",
    "ujjain",
    "unittest",
    "verstr",
    "verstrline",
    "vsre",
    "webapp"
  ],
  "cSpell.ignoreWords": [
    "jvn",
    "mycompany",
    "notif",
    "seealso"
  ],
  "editor.formatOnSave": true,
  "prettier.arrowParens": "avoid",
  "prettier.trailingComma": "none",
  "prettier.disableLanguages": ["html"],
  "html.format.wrapLineLength": 50,
  "html.format.wrapAttributes": "auto",
  "python.pythonPath": "${env:JOVIAN_PY_DEV_PYTHONPATH}"

}
